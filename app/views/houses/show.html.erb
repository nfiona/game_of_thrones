
<h1> <%= @house.name %></h1>
<h3> "<%= @house.slogan %>"</h3>


  <% @house.characters.each do |character| %>
      <h2 class="character_names"><%= link_to character.name, house_character_path(@house,character) %></h2>
  <% end %>

<h4> <%= link_to "Edit House", edit_house_path(@house) %> | <%= link_to "Add Character", new_house_character_path(@house) %> </h4>
